{
  "name": "foundry-action",
  "version": "1.0.0",
  "description": "Foundry: Deterministic repository creation for platform teams",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "compile": "tsc",
    "format": "prettier --write '**/*.{ts,js,json,md}'",
    "format:check": "prettier --check '**/*.{ts,js,json,md}'",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "package": "ncc build src/index.ts -o dist --source-map --license licenses.txt",
    "test": "jest",
    "all": "npm run format && npm run lint && npm run build && npm run package",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/foundry.git"
  },
  "keywords": [
    "github-action",
    "repository",
    "scaffolding",
    "octokit",
    "platform-engineering"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/github": "^6.0.0",
    "@octokit/rest": "^20.0.2",
    "loglevel": "^1.9.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vercel/ncc": "^0.38.1",
    "eslint": "^8.56.0",
    "eslint-plugin-github": "^4.10.1",
    "eslint-plugin-jest": "^27.6.0",
    "gts": "^5.3.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.9.3"
  }
}
